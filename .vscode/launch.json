{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/selfbuild/main.selfbuild",
            "args": [
                "../hello.c",
                "-I",
                "../lib",
                "-c",
                "-D",
                "__x86_64__",
                "-D",
                "__STDC__",
                "-D",
                "_POSIX_SOURCE",
                "-D",
                "__DARWIN_OS_INLINE=static inline",
                "-D",
                "__llvm__",
                "-D",
                "__builtin_bswap32",
                "-D",
                "__builtin_bswap16",
                "-D",
                "__builtin_bswap64",
                "-D",
                "__LP64__"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/out",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": []
        },
        {
            "name": "(gdb) main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/out/main",
            "args": [
                "../lib/be.c",
                "-I",
                "../lib",
                "-c",
                "-o",
                "be.asm"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/out",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": []
        }
    ]
}